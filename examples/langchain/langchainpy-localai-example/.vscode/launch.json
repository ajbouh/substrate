{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "redirectOutput": true,
            "justMyCode": false
        },
        {
            "name": "Python: Attach to Port 5678",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
              },
            "justMyCode": false
        }
    ]
}